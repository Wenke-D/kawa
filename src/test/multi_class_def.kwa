/* multiple class definition with certain same methods */

class point {
  attribute int x;
  attribute int y;

  method void constructor(int a, int b) {
    this.x = a;
    this.y = b;
  }

  method int somme() {
    return this.x + this.y;
  }

  method int x(){
    return this.x;
  }
}

class square{
  attribute int x;
  attribute int y;
  attribute int length;

  method void constructor(int a, int b, int length) {
    this.x = a;
    this.y = b;
    this.length = length;
  }

  method int somme() {
    return this.x + this.y;
  }

  method int x(){
    return this.x;
  }

  method int length(){
    return this.length;
  }
}



